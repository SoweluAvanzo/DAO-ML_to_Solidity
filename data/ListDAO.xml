<DAO-ML_diagram xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="XSD_TDM_Decentralized_Organization.xsd" name="Group Currency DAO Diagram" uniqueID="GCDAOdiagram" >
    <DAO DAO_ID="LIST" DAO_name="List Test DAO" mission_statement="Managing the governance of Group Currencies in the Circles UBI system." hierarchical_inheritance="1">
        <Role role_ID="GroupMember" role_name="Group Member" role_assignment_method="depositing Circles tokens" agent_type="human">
            <aggregates>CC</aggregates>
        </Role>
        <Role role_ID="ActiveMember" role_name="Active Member" role_assignment_method="Perishable Share ownership" agent_type="human">
            <aggregates>BB</aggregates>
            <aggregates>AA</aggregates>
            
        </Role>
        <Committee committee_ID="CC" committee_description="General Assembly" appointment_method="group currency ownership">
            <is_controlled_by>BB</is_controlled_by>
        </Committee>
    
        <Committee committee_ID="BB" committee_description="Community Council" appointment_method="rotation">
            <is_controlled_by>AA</is_controlled_by>


        </Committee>
        <Committee committee_ID="AA" committee_description="Technical Council" n_agent_min="1" n_agent_max="7" appointment_method="election">
        </Committee>
    </DAO>
</DAO-ML_diagram>
